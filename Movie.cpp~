#include <iostream>
#include <cstring>
#include "Movie.h"

using namespace std;

void Movie::setRating(float rate){
  rating = rate;
}
float Movie::getRating(){
  return rating;
}
void Movie::setDirector(char name[30]){
  director = new char[30];
  strcpy(director, name);
}
char Movie::getDirector(){
  return director;
}
void Movie::setDuration(int time){
  duration = time;
}
int Movie::setDuration(){
  return duration;
};
